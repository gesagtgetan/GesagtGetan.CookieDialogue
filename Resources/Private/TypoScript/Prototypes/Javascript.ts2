prototype(GesagtGetan.CookieDialogue:Javascript) < prototype(TYPO3.TypoScript:Tag) {
    configuration = ${Configuration.setting('GesagtGetan.CookieDialogue.javascript')}
    path = ${'resource://GesagtGetan.CookieDialogue/Private/Templates/' + this.configuration.implementation + '.js'}
    @if.render = ${this.configuration.include}
    @context.path = ${this.path}

    tagName = 'script'
    content = Carbon.FileContent:FileContent {
        path = ${path}
    }
}
