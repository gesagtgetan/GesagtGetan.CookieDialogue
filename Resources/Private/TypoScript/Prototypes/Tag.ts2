// Namespaces are defined in the Root.ts2 file

prototype(C:Tag) < prototype(F:Tag) {
	tagName = 'script'
	file = ${'resource://GesagtGetan.CookieDialogue/Public/Main.' + (this.tagName == 'script' ? 'js' : 'css')}
	@context.file = ${this.file}
	@if.file = ${!!this.file}

	content = C:FileContent {
		path = ${file}
	}
}
