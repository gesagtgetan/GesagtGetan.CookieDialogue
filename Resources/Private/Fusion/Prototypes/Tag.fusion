prototype(GesagtGetan.CookieDialogue:Tag) < prototype(Neos.Fusion:Tag) {
    tagName = 'script'
    file = ${'resource://GesagtGetan.CookieDialogue/Public/Main.' + (this.tagName == 'script' ? 'js' : 'css')}
    @context.file = ${this.file}
    @if.file = ${!!this.file}

    content = Carbon.FileContent:FileContent {
        path = ${file}
    }
}
